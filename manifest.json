{
	"manifest_version": 3,
	"name": "oo.pe skip",
	"version": "1.0",
	"description": "Skip oo.pe warning",
  
	"icons": {
	  "16": "images/icon-16.png",
	  "32": "images/icon-32.png",
	  "48": "images/icon-48.png",
	  "128": "images/icon-128.png"
	},
	"content_scripts": [
	  {
		"js": [
		  "scripts/content.js"
		],
		"matches": [
		  "https://oo.pe/*"
		]
	  }
	]
  }